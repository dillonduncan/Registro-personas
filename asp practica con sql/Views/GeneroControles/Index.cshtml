@model List<TablaGenero>

<h1>Generos</h1>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var genero in Model)
        {
            <tr>
                <td>@Html.DisplayFor(a=>genero.Id)</td>
                <td>@Html.DisplayFor(a=>genero.NombreGenero)</td>
            </tr>
        }
    </tbody>
</table>